ass=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-lg-8 mx-auto mbr-form\">\n\n                  <form class=\"mbr-form anphabe-lead form-with-styler  needs-validation mt-5 pt-5\" novalidate data-form-type=\"anphabe-lead\" name=\"lien-he-VNBPTW\" id=\"lien_he\" method=\"POST\">      \n                                  <input type=\"hidden\" id=\"product_name-form7-44\" name=\"product_name\" data-form-field=\"product_name\" value=\"VNBPTW\">\n                                  <input type=\"hidden\" name=\"landingpage\" value=\"VNBPTW\">\n                    <input type=\"hidden\" id=\"utm_campaign-form7-44\" name=\"utm_campaign\" data-form-field=\"utm_campaign\" value>\n                    <input type=\"hidden\" id=\"utm_source-form7-44\" name=\"utm_source\" data-form-field=\"utm_source\" value>\n                    <input type=\"hidden\" id=\"utm_medium-form7-44\" name=\"utm_medium\" data-form-field=\"utm_medium\" value>\n                    <input type=\"hidden\" id=\"utm_term-form7-44\" name=\"utm_term\" data-form-field=\"utm_term\" value>\n                    <input type=\"hidden\" id=\"utm_content-form7-44\" name=\"utm_content\" data-form-field=\"utm_content\" value>\n                    <a id=\"destination\" href=\"thankyou.html\" style=\"display:none;\"></a>\n                                  <div class=\"dragArea row\">\n                                      <div class=\"col-12\" mbr-if=\"showTitle\">\n                                          <h4 class=\"mbr-section-title mbr-fonts-style align-center mb-4 mt-2 pt-2 mt-md-5 pt-md-5\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">ĐĂNG KÝ TRỞ THÀNH ỨNG VIÊN DANH GIÁ CỦA NƠI LÀM VIỆC TỐT NHẤT VIỆT NAM</h4>\n                                      </div>\n                                      <div class=\"col-12\">\n                                          <p mbr-theme-style=\"display-7\" class=\"mbr-text mbr-fonts-style mb-5\" mbr-if=\"showText\">Điện thoại:&nbsp; <a href=\"tel:+842862682222\" class=\"text-secondary\">(+84) 28 6268 2222</a><br>\nEmail:&nbsp;<a href=\"mailto:clientsolution@anphabe.com\" class=\"text-secondary\">clientsolution@anphabe.com</a></p>\n                                      </div>\n\n                                      <div class=\"col-sm-12 form-group mb-3\" data-for=\"name\">\n                                          <input type=\"text\" name=\"fullname\" placeholder=\"Họ và tên bạn\" data-form-field=\"fullname\" class=\"form-control\" value required>\n                                            <div class=\"invalid-feedback text-start  mbr-fonts-style\">Vui lòng nhập tên của bạn</div>\n                                      </div>\n                                      <div class=\"col-md col-sm-12 form-group mb-3\" data-for=\"phone\">\n                                          <input type=\"text\" name=\"phone\" placeholder=\"Điện thoại liên lạc\" data-form-field=\"phone\" class=\"form-control\" value required>\n                                          <div class=\"invalid-feedback text-start mbr-fonts-style\">Vui lòng nhập số điện thoại của bạn</div>\n                                      </div>\n                                      <div class=\"col-md col-sm-12 form-group mb-3\" data-for=\"email\">\n                                          <input type=\"email\" name=\"email\" placeholder=\"E-mail liên lạc\" data-form-field=\"email\" class=\"form-control\" value required>\n                                          <div class=\"invalid-feedback text-start mbr-fonts-style\">Vui lòng nhập email của bạn</div>\n                                      </div>\n                                      <div class=\"col-sm-12 form-group mb-3\" data-for=\"position\">\n                                          <input type=\"text\" name=\"position\" placeholder=\"Chức vụ\" data-form-field=\"position\" class=\"form-control\" value required>\n                                          <div class=\"invalid-feedback text-start mbr-fonts-style\">Vui lòng nhập chức vụ của bạn</div>\n                                      </div>\n                                      <div class=\"col-sm-12 form-group mb-3\" data-for=\"companyname\">\n                                          <input type=\"text\" name=\"companyname\" placeholder=\"Tên công ty\" data-form-field=\"companyname\" class=\"form-control\" value required>\n                                          <div class=\"invalid-feedback text-start mbr-fonts-style\">Vui lòng nhập tên công ty</div>\n                                      </div>\n                                      <div class=\"col-sm-12 form-group mb-3\" data-for=\"company-size\">\n                                        <select class=\"form-control\" title=\"company-size\" name=\"company-size\" data-form-field=\"company-size\" required>\n                                          <option value selected>-- Quy mô nhân sự --</option>\n                                        <option value=\"1-50 nhân sự\">1-60 nhân sự</option>\n                                        <option value=\"51-200 nhân sự\">61-200 nhân sự</option>\n                                        <option value=\"201-500 nhân sự\">201-500 nhân sự</option>\n                                        <option value=\"501-1000 nhân sự\">501-1000 nhân sự</option>\n                                        <option value=\"Hơn 1000 nhân sự\">Hơn 1000 nhân sự</option>\n                                        </select>\n                                        <div class=\"invalid-feedback text-start mbr-fonts-style\">Vui lòng chọn quy mô nhân sự</div>\n                                      </div>\n            \n                                      <div class=\"col-12 form-group mb-3\" data-for=\"nhucautuvan-detail\">\n                                          <textarea name=\"nhucautuvan-detail\" data-form-field=\"nhucautuvan-detail\" placeholder=\"Vui lòng mô tả nhu cầu cần tư vấn của anh/chị!\" class=\"form-control\"></textarea>\n                                      </div>\n                                      <div mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"col-lg-12 col-md-12 col-sm-12 align-center mbr-section-btn display-4\"><button type=\"submit\" class=\"btn btn-warning click-tracking\" data-tracking-action=\"gui-thong-tin\" data-app-placeholder=\"Type Text\"><span class=\"mobi-mbri mobi-mbri-arrow-next mbr-iconfont mbr-iconfont-btn\" style=\"font-size: 12px;\"></span>GỬI THÔNG TIN</button></div>\n                                  </div>\n                              </form>\n\n\n\n\n            </div>\n        </div>\n    </div>\n    <div class=\"d-none\">\n        [JS_CODE_0]\n    </div>\n</section>",
